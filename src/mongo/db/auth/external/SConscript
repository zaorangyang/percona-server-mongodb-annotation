# -*- mode: python -*-
Import("env")

env = env.Clone()

env.Library(
    target='externalsaslauth',
    source=[
        'cyrus_sasl_server_session.cpp',
        'external_sasl_authentication_session.cpp',
        'gssapi_server_mechanism.cpp',
    ],
    LIBDEPS=[
        '$BUILD_DIR/mongo/db/auth/saslauth'
    ],
    SYSLIBDEPS=[
        'gssapi_krb5',
        'sasl2'
    ],
)
